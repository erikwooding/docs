{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Meter",
  "colors": {
    "primary": "#5361C7",
    "light": "#ffffff",
    "dark": "#5462c7"
  },
  "favicon": "/favicon.png",
  "logo": {
    "light": "https://mintlify.s3.us-west-1.amazonaws.com/meter-preview/logo/logo.svg",
    "dark": "https://mintlify.s3.us-west-1.amazonaws.com/meter-preview/logo/logo.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      },
      {
        "tab": "Dashboard",
        "groups": [
          {
            "group": "Insights",
            "pages": [
              "dashboard/insights/network",
              "dashboard/insights/clients-wifi",
              "dashboard/insights/clients-ip"
            ]
          },
          {
            "group": "Hardware",
            "pages": [
              "dashboard/hardware/access-points",
              "dashboard/hardware/access-points-bssids",
              "dashboard/hardware/access-points-clients"
            ]
          }
        ]
      },
      {
        "tab": "General",
        "groups": [
          {
            "group": "Troubleshooting Documentation",
            "pages": [
              "general/troubleshooting-documentation/troubleshooting-connectivity",
              "general/troubleshooting-documentation/meter-speed-test",
              "general/troubleshooting-documentation/gather-har-file"
            ]
          },
          {
            "group": "FAQ",
            "pages": [
              "general/faq/welcome-meter-support",
              "general/faq/lte-connection-isp-outage",
              "general/faq/isp-throughput"
            ]
          }
        ]
      },
      {
        "tab": "WiFi",
        "groups": [
          {
            "group": "Troubleshooting",
            "pages": [
              "wifi/troubleshooting/troubleshooting-collect-system-info",
              "wifi/troubleshooting/troubleshooting-wifi-speeds-slow",
              "wifi/troubleshooting/troubleshooting-which-access-point"
            ]
          },
          {
            "group": "FAQ",
            "pages": [
              "wifi/faq/expected-wifi-speeds",
              "wifi/faq/connect-to-meter-wifi-network",
              "wifi/faq/what-is-rssi"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        },
        {
          "anchor": "Meter Blog",
          "href": "https://www.meter.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@meter.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.meter.com/support"
    }
  },
  "favicon": "/favicon.svg",
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/meter",
      "github": "https://github.com/mintlify",
      "linkedin": "https://www.linkedin.com/company/meter-com"
    }
  },
  "fonts": {
    "heading": {
      "family": "SuisseIntl-Bold",
      "source": "https://mintlify-assets.b-cdn.net/suisse-intl-600.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "SuisseIntl-Regular",
      "source": "https://mintlify-assets.b-cdn.net/suisse-intl-400.woff2",
      "format": "woff2"
    }
  },
  "styling": {
    "codeblocks": "system"
  }
}
